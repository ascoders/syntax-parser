  },
  "devDependencies": {
    "pri": "3.0.0"
  },
  "main": "dist/main",
  "types": "declaration/index.d.ts",
  "peerDependencies": {},
  "dependencies": {
    "@babel/runtime": "7.7.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test -- --package root"
    }
  },
  "module": "dist/module"
}
